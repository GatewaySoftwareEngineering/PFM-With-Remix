{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "dev": "concurrently \"yarn assets:dev\" \"yarn sass:dev\" \"yarn remix dev\"",
    "assets": "npx @svgr/cli --out-dir app/shared/assets/ -- assets",
    "assets:watch": "npm-watch assets",
    "assets:dev": "concurrently --hide \"yarn assets\" \"yarn assets:watch\"",
    "sass:dev": "sass -q --no-source-map --watch styles/:app/styles",
    "sass:build": "sass styles/:app/styles",
    "build": "yarn sass:build && remix build",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "concurrently": "^7.6.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "isbot": "^3.6.5",
    "react": "^18.2.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.2.0",
    "sass": "^1.57.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@svgr/cli": "^6.5.1",
    "@svgr/plugin-jsx": "^6.5.1",
    "@svgr/plugin-prettier": "^6.5.1",
    "@svgr/plugin-svgo": "^6.5.1",
    "eslint": "^8.31.0",
    "npm-watch": "^0.11.0"
  },
  "engines": {
    "node": ">=14"
  },
  "watch": {
    "assets": {
      "patterns": [
        "assets"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
